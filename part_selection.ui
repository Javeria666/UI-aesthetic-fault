  <property name="styleSheet">
   <string notr="true">/* ---- Overall Style ---- */
#PartSelectionScreen {
	background-color: #f0f2f5; /* Light grey background */
	font-family: Arial, sans-serif;
}

/* ---- Header Style ---- */
#headerFrame {
	background-color: #1976d2; /* Professional blue */
	border-radius: 0px;
}
#titleLabel {
	color: white;
	font-size: 20pt;
	font-weight: bold;
}
#loginButton {
	background-color: white;
	color: #1976d2;
	font-size: 12pt;
	font-weight: bold;
	padding: 10px 20px;
	border-radius: 8px;
	border: none;
}
#loginButton:hover {
	background-color: #e3f2fd; /* Light blue on hover */
}

/* ---- Part Selection Area ---- */
QScrollArea {
	border: none;
}
#scrollAreaWidgetContents {
	background-color: #f0f2f5; /* Match the main background */
}

/* --- THIS IS THE FIX --- */
#gridFrame {
	background-color: transparent;
	border: none;
}
/* ------------------------ */

QToolButton {
	background-color: white;
	border: 2px solid #e0e0e0; /* Default light grey border */
	border-radius: 8px;
	padding: 10px;
	color: #333;
	font-size: 11pt;
	font-weight: bold;
}
QToolButton:hover {
	border-color: #1976d2; /* Blue border on hover */
}
QToolButton:checked {
	border: 3px solid #0d47a1; /* Darker blue solid border when selected */
	background-color: #e3f2fd; /* Light blue background when selected */
}

/* ---- Footer Style ---- */
#footerFrame {
	background-color: #ffffff;
	border-top: 1px solid #e0e0e0;
}
#selectButton {
	background-color: #1976d2;
	color: white;
	font-size: 12pt;
	font-weight: bold;
	padding: 10px 20px;
	border-radius: 8px;
	border: none;
	min-width: 120px;
}
#selectButton:hover {
	background-color: #1565c0; /* Darker blue */
}</string>
